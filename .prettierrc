{
  "singleQuote": true,
  "trailingComma": "all",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "@nestjs/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@configs/(.*)$",
    "^@shared/(.*)$",
    "^@modules/(.*)$",
    "^@src/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": [
    "classProperties",
    "decorators-legacy",
    "typescript"
  ]
}
